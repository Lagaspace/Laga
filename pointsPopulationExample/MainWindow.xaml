<Window x:Class="dblPopulationExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dblPopulationExample"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Population Example" Height="398" Loaded="Window_Loaded" Width="800">
    <Grid>
        <StackPanel Margin="2" Background="#FF11162E" Orientation="Horizontal">
            <TextBox x:Name="txtdblPop" Margin="5" ScrollViewer.VerticalScrollBarVisibility="Auto" Width="770" TextWrapping="Wrap" Text="POINTS POPULATION : " Background="{x:Null}" FontSize="14" Foreground="#FFA2A0A0" SelectionBrush="#FF8D8D8D" BorderBrush="#FF334B91"/>
        </StackPanel>

    </Grid>
</Window>
