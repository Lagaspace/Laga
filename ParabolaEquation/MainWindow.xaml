<Window x:Name="noteBook" x:Class="ParabolaEquation.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParabolaEquation"
        mc:Ignorable="d"
        Title="Parabola Equation notes" Height="400" Width="939" Loaded="noteBook_Loaded">
    <Grid>
        <TextBox x:Name="textBox" Margin="18,41,0,0" Width ="80" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" AcceptsTab="True" BorderBrush="#FF2D50B9" HorizontalAlignment="Left" Height="288" VerticalAlignment="Top" />
        <TextBox x:Name="txtEvolve" Margin="108,41,0,0" Width ="160" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" AcceptsTab="True" BorderBrush="#FF3552A6" HorizontalAlignment="Left" Height="288" VerticalAlignment="Top" />
        <TextBox x:Name="txtSonAndMutation" Margin="565,41,0,0" Width="80" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" AcceptsTab="True" BorderBrush="#FF3552A6" HorizontalAlignment="Left" Height="120" VerticalAlignment="Top" />
        <Label x:Name="label" Content="original pop -&gt;" HorizontalAlignment="Left" Margin="13,15,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="evaluation" HorizontalAlignment="Left" Margin="103,15,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtSonAndMutation_Copy" Margin="565,192,0,0" Width="80" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" AcceptsTab="True" BorderBrush="#FF3552A6" HorizontalAlignment="Left" Height="137" VerticalAlignment="Top" />
        <Label x:Name="label2" Content="crossover -&gt;" HorizontalAlignment="Left" Margin="560,15,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy" Content="sons -&gt;" HorizontalAlignment="Left" Margin="560,166,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtEvolve_Copy" Margin="655,41,0,0" Width ="160" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" AcceptsTab="True" BorderBrush="#FF3552A6" HorizontalAlignment="Left" Height="288" VerticalAlignment="Top" />
        <Label x:Name="label3" Content="Replacement" HorizontalAlignment="Left" Margin="650,15,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label4" Content="best = 9?" HorizontalAlignment="Left" Margin="273,167,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.193,0.187"/>
        <TextBox x:Name="txtEvolve_Copy1" Margin="465,41,0,0" Width ="90" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" AcceptsTab="True" BorderBrush="#FF3552A6" HorizontalAlignment="Left" Height="288" VerticalAlignment="Top" />
        <Label x:Name="label1_Copy" Content="selection -&gt;" HorizontalAlignment="Left" Margin="460,15,0,0" VerticalAlignment="Top"/>
        <Line X1="180" Y1="350"  X2="730" Y2="350" Stroke="#FF3552A6" StrokeThickness="2"  />
        <Line X1="180" Y1="350"  X2="180" Y2="330" Stroke="#FF3552A6" StrokeThickness="2" />
        <Line X1="730" Y1="350"  X2="730" Y2="330" Stroke="#FF3552A6" StrokeThickness="2" />
        <Button x:Name="btnRun" Content="Run" HorizontalAlignment="Left" Margin="278,216,0,0" VerticalAlignment="Top" Width="75" Click="btnRun_Click"/>
        <Button x:Name="btnStop" Content="Stop" HorizontalAlignment="Left" Margin="278,246,0,0" VerticalAlignment="Top" Width="75" Height="20" Click="btnStop_Click"/>
    </Grid>
</Window>
